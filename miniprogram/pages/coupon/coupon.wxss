/* pages/coupon/coupon.wxss */

Page {
  width: 100%;
  height: 100%;
}

.whole-block {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.dui-huan {
  width: 100%;
  height: 90rpx;
  display: flex;
}

.btn {
  width: 250rpx;
  height: 100%;
  border-radius: 4px;
  background-color: rgba(16, 136, 162, 1);
  color: rgba(255, 255, 255, 1);
  font-size: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid rgba(187, 187, 187, 1);
}

.coupon {
  width: 100%;
  height: 120rpx;
  margin-top: 25rpx;
  background-color: rgba(16, 136, 162, 1);
  color: rgba(16, 16, 16, 1);
  font-size: 20px;
  display: flex;
  align-items: center;
}

.nobtn{
width: 100%;
height: 80rpx;
bottom: 0rpx;
position: absolute;
border-radius: 4px;
background-color: rgba(206, 26, 26, 1);
color: rgba(255, 255, 255, 1);
font-size: 20px;
display: flex;
align-items: center;
justify-content: center;
border: 1px solid rgba(187, 187, 187, 1);
}